import java.util.Scanner;
class HelloWorld {
public static void main(String[] args) {
int num, digit, sum=0;
Scanner input=new Scanner(System.in);
System.out.println("Input a number: ");
num= input.nextInt();
while(num>0)
{
digit=num%10;
sum=sum+digit;
num=num/10;
}
System.out.print("Sum of the digit is "+sum);
}
}
